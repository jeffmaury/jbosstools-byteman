# Test for BYTEMAN-246 to ensure that boolena comparisons give the expected results

RULE check boolean comparisons
CLASS TestBooleanComparisons
METHOD triggerMethod
BIND test : Test = $0;
IF TRUE
DO test.log("b1 == b1 ==> " + ($1 == $1));
   test.log("b1 == b2 ==> " + ($1 == $2));
   test.log("b1 != b1 ==> " + ($1 != $1));
   test.log("b1 != b2 ==> " + ($1 != $2));
   test.log("b2 == b2 ==> " + ($2 == $2));
   test.log("b2 == b1 ==> " + ($2 == $1));
   test.log("b2 != b2 ==> " + ($2 != $2));
   test.log("b2 != b1 ==> " + ($2 != $1));

   test.log("B1 == B1 ==> " + ($3 == $3));
   test.log("B1 == B2 ==> " + ($3 == $4));
   test.log("B1 != B1 ==> " + ($3 != $3));
   test.log("B1 != B2 ==> " + ($3 != $4));
   test.log("B2 == B2 ==> " + ($4 == $4));
   test.log("B2 == B1 ==> " + ($4 == $3));
   test.log("B2 != B2 ==> " + ($4 != $4));
   test.log("B2 != B1 ==> " + ($4 != $3));

   test.log("b1 == B1 ==> " + ($1 == $3));
   test.log("b1 == B2 ==> " + ($1 == $4));
   test.log("b1 != B1 ==> " + ($1 != $3));
   test.log("b1 != B2 ==> " + ($1 != $4));
   test.log("B2 == b2 ==> " + ($4 == $2));
   test.log("B2 == b1 ==> " + ($4 == $1));
   test.log("B2 != b2 ==> " + ($4 != $2));
   test.log("B2 != b1 ==> " + ($4 != $1));
ENDRULE